{
  "name": "xgplayer",
  "version": "0.0.1",
  "description": "xgplayer for video player",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:hls": "yarn dev:base --environment target:hls",
    "dev:flv": "yarn dev:base --environment target:flv",
    "dev:dash": "yarn dev:base --environment target:dash",
    "dev:base": "rollup -wc ./scripts/rollup.dev.js",
    "test:watch": "jest --watch",
    "bootstrap": "npm install & lerna bootstrap",
    "watch": "http-server --cors='Access-Control-Allow-Origin: *' & lerna run --parallel watch",
    "watch:live": "http-server -S -C cert.pem --cors='Access-Control-Allow-Origin: *' & lerna run --scope xgplayer-flv-live --scope xgplayer-hls-live --scope xgplayer-helper-* --parallel watch",
    "test": "node_modules/.bin/lerna-changelog",
    "dev": "npm run watch",
    "publish": "lerna publish",
    "publish:live": "sh scripts/publishTransmuxers.sh",
    "cz": "git cz",
    "precz": "lint-staged",
    "remove": "lerna run clean",
    "clean": "lerna clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance/xgplayer.git"
  },
  "keywords": [
    "xgplayer",
    "mp4",
    "hls",
    "flv",
    "dash"
  ],
  "lint-staged": {
    "linters": {
      "packages/**/*.js": [
        "eslint -c .eslintrc.js --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/browser/*.js",
      "**/dist/*.js"
    ]
  },
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bytedance/xgplayer/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "homepage": "https://github.com/bytedance/xgplayer#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/parser": "^7.14.4",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/runtime": "^7.14.0",
    "@babel/runtime-corejs3": "^7.14.0",
    "@babel/traverse": "^7.14.2",
    "@babel/types": "^7.15.0",
    "@koa/cors": "^2.2.1",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/events": "^3.0.0",
    "autoprefixer": "^9.1.5",
    "babel-jest": "^27.0.6",
    "babel-plugin-inline-svg": "^1.2.0",
    "babel-plugin-search-and-replace": "^1.1.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.2",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^3.1.0",
    "fs-extra": "^5.0.0",
    "generate-source-map": "0.0.5",
    "glob": "^7.1.2",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "json-loader": "^0.5.7",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.0-rc.1",
    "koa": "^2.5.1",
    "koa-range": "^0.3.0",
    "koa-static": "^4.0.3",
    "lerna": "^3.17.0",
    "lerna-changelog": "^0.8.0",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "recursive-readdir": "^2.2.2",
    "rollup": "^2.48.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-url": "^3.0.0",
    "rollup-plugin-visualizer": "^5.5.0",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "sugarss": "^1.0.1",
    "terser": "^5.7.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^1.1.1",
    "url-toolkit": "2.1.6",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2",
    "webworkify-webpack": "2.1.5",
    "webworkify-webpack-dropin": "^1.1.9",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "xgplayer": "file:packages/xgplayer",
    "xgplayer-dash": "file:packages/xgplayer-dash",
    "xgplayer-flv-live": "file:packages/xgplayer-flv-live",
    "xgplayer-flv-vod": "file:packages/xgplayer-flv-vod",
    "xgplayer-flv.js": "file:packages/xgplayer-flv.js",
    "xgplayer-helper-codec": "file:packages/xgplayer-helper-codec",
    "xgplayer-helper-models": "file:packages/xgplayer-helper-models",
    "xgplayer-helper-transmuxers": "file:packages/xgplayer-helper-transmuxers",
    "xgplayer-helper-utils": "file:packages/xgplayer-helper-utils",
    "xgplayer-hls-live": "file:packages/xgplayer-hls-live",
    "xgplayer-hls-vod": "file:packages/xgplayer-hls-vod",
    "xgplayer-hls.js": "file:packages/xgplayer-hls.js",
    "xgplayer-livevideo": "file:packages/xgplayer-livevideo",
    "xgplayer-m4a": "file:packages/xgplayer-m4a",
    "xgplayer-mp4": "file:packages/xgplayer-mp4",
    "xgplayer-music": "file:packages/xgplayer-music",
    "xgplayer-raw264": "file:packages/xgplayer-raw264",
    "xgplayer-render": "file:packages/xgplayer-render",
    "xgplayer-shaka": "file:packages/xgplayer-shaka"
  }
}
