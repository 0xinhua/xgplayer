{
  "name": "xgplayer-m4a",
  "version": "1.1.5",
  "description": "xgplayer plugin for m4a transform to fmp4",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false,
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "keywords": [
    "mp4",
    "fmp4",
    "player",
    "audio"
  ],
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "concat-typed-array": "^1.0.2",
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5",
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  }
}
