{
  "name": "xgplayer-hls",
  "version": "3.0.0-alpha.16",
  "description": "xgplayer plugin for hls",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "browser": "./dist/index.min.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "babel src --out-dir es",
    "build:lib": "babel src --out-dir lib",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "keywords": [
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "unpkgFiles": [
    "dist"
  ],
  "dependencies": {
    "xgplayer-hls-live": "3.0.0-alpha.16",
    "xgplayer-hls-vod": "3.0.0-alpha.16"
  }
}
