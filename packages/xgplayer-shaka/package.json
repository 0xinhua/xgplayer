{
  "name": "xgplayer-shaka",
  "version": "1.1.5",
  "description": "xgplayer plugin for shaka-player",
  "main": "dist/index.min.js",
  "module": "es/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ]
  },
  "keywords": [
    "mpeg-dash",
    "shaka-player",
    "player",
    "video"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance/xgplayer.git"
  },
  "author": "zhangxin.frontend@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^6.0.3",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-uglify-es": "^0.0.1",
    "shaka-player": "^2.4.0"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  }
}
