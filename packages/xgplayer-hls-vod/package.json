{
  "name": "xgplayer-hls-vod",
  "version": "3.0.0-alpha.123-1",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "babel src --out-dir es && cp types/es.d.ts es/index.d.ts",
    "build:lib": "babel src --presets @babel/preset-env --out-dir lib",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch",
    "watch:mobile": "cross-env NODE_ENV=development rollup -c rollup.config.mobile.js --watch"
  },
  "author": "",
  "license": "ISC",
  "unpkgFiles": [
    "dist"
  ],
  "dependencies": {
    "xgplayer-helper-codec": "2.4.33-beta.1",
    "xgplayer-helper-models": "2.4.33-beta.1",
    "xgplayer-helper-transmuxers": "2.4.33-beta.1",
    "xgplayer-helper-utils": "2.4.33-beta.1"
  },
  "peerDependencies": {
    "xgplayer": "3.0.0-alpha.89"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "search-and-replace",
        {
          "rules": [
            {
              "search": "__VERSION__",
              "searchTemplateStrings": true,
              "replace": "3.0.0-alpha.123-1"
            }
          ]
        }
      ]
    ]
  }
}
