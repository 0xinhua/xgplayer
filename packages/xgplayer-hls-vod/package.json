{
  "name": "xgplayer-hls-vod",
  "version": "3.0.0-alpha.0",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "babel src --out-dir es",
    "build:lib": "babel src --out-dir lib --presets=es2015",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "author": "",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false,
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "license": "ISC",
  "dependencies": {
    "xgplayer-utils": "^3.0.0-alpha.0",
    "xgplayer-utils-crypto": "^3.0.0-alpha.0",
    "xgplayer-utils-mse": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-constant-events": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-context": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-buffer-track": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-buffer-presource": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-buffer-xgbuffer": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-loader-fetch": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-remux-mp4": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-codec-compatibility": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-demux-m3u8": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-demux-ts": "^3.0.0-alpha.0",
    "xgplayer-transmuxer-buffer-playlist": "^3.0.0-alpha.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  },
  "peerDependencies": {
    "xgplayer": "3.0.0-alpha.0"
  }
}
