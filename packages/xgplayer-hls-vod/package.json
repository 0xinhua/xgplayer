{
  "name": "xgplayer-hls-vod",
  "version": "2.2.0-alpha.6",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "babel src --out-dir es",
    "build:lib": "babel src --out-dir lib --presets=es2015",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "author": "",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false,
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "license": "ISC",
  "dependencies": {
    "cross-env": "^6.0.3",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-uglify-es": "^0.0.1",
    "xgplayer-transmuxer-buffer-playlist": "file:../xgplayer-transmuxer-buffer-playlist",
    "xgplayer-transmuxer-buffer-presource": "file:../xgplayer-transmuxer-buffer-presource",
    "xgplayer-transmuxer-buffer-track": "file:../xgplayer-transmuxer-buffer-track",
    "xgplayer-transmuxer-buffer-xgbuffer": "file:../xgplayer-transmuxer-buffer-xgbuffer",
    "xgplayer-transmuxer-codec-compatibility": "file:../xgplayer-transmuxer-codec-compatibility",
    "xgplayer-transmuxer-constant-events": "file:../xgplayer-transmuxer-constant-events",
    "xgplayer-transmuxer-context": "file:../xgplayer-transmuxer-context",
    "xgplayer-transmuxer-demux-m3u8": "file:../xgplayer-transmuxer-demux-m3u8",
    "xgplayer-transmuxer-demux-ts": "file:../xgplayer-transmuxer-demux-ts",
    "xgplayer-transmuxer-loader-fetch": "file:../xgplayer-transmuxer-loader-fetch",
    "xgplayer-transmuxer-remux-mp4": "file:../xgplayer-transmuxer-remux-mp4",
    "xgplayer-utils": "file:../xgplayer-utils",
    "xgplayer-utils-crypto": "file:../xgplayer-utils-crypto",
    "xgplayer-utils-mse": "file:../xgplayer-utils-mse"
  }
}
