{
  "name": "xgplayer-flv.js",
  "version": "2.1.1",
  "description": "web video player",
  "main": "./dist/index.js",
  "scripts": {
    "init": "npm install",
    "clean": "rm -rf dist browser",
    "prepare": "npm run build",
    "dev": "webpack --config ./webpack.config.dev.js",
    "build": "webpack --progress --display-chunks -p",
    "watch": "webpack --progress --display-chunks -p --watch"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "babel-plugin-bulk-import"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance/xgplayer.git"
  },
  "author": "cuc_ygh@163.com",
  "license": "ISC",
  "peerDependency": {
    "xgplayer": "^0.1.0"
  },
  "dependencies": {
    "cross-env": "^6.0.3",
    "es6-promise": "^4.2.4",
    "glob": "^7.1.2",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-uglify-es": "^0.0.1",
    "webworkify": "^1.5.0"
  }
}
