{
  "name": "xgplayer-dash",
  "version": "1.1.6",
  "description": "xgplayer plugin for MPEG-DASH",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers",
      "babel-plugin-bulk-import"
    ]
  },
  "keywords": [
    "MPEG-DASH",
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "zhangxin.frontend@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5",
    "xml2js": "^0.4.19"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-bulk-import": "^1.0.2",
    "babel-preset-es2015": "^6.24.1"
  }
}
