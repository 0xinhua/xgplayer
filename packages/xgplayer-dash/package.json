{
  "name": "xgplayer-dash",
  "version": "3.0.0-alpha.9",
  "description": "xgplayer plugin for MPEG-DASH",
  "main": "./dist/index.js",
  "browser": "./browser/index.js",
  "module": "./es/index.js",
  "scripts": {
    "build:es": "babel src --out-dir es",
    "build:lib": "babel src --out-dir lib --presets=es2015",
    "build": "webpack --progress --display-chunks -p",
    "watch": "webpack --progress --display-chunks --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "babel-plugin-bulk-import"
    ]
  },
  "keywords": [
    "MPEG-DASH",
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "zhangxin.frontend@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^6.0.3",
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-uglify-es": "^0.0.1",
    "xml2js": "^0.4.19"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  },
  "devDependencies": {
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
