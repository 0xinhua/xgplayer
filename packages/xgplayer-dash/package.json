{
  "name": "xgplayer-dash",
  "version": "1.1.6",
  "description": "xgplayer plugin for MPEG-DASH",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "webpack --progress --display-chunks -p",
    "watch": "webpack --progress --display-chunks -p --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "babel-plugin-bulk-import"
    ]
  },
  "keywords": [
    "MPEG-DASH",
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "zhangxin.frontend@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^6.0.3",
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-uglify-es": "^0.0.1",
    "xml2js": "^0.4.19"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  }
}
