{
  "name": "xgplayer-render",
  "version": "3.0.0-alpha.16",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "babel src --out-dir es",
    "build:lib": "babel src --out-dir lib --presets=es2015",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false
        }
      ]
    ]
  }
}
