{
  "name": "xgplayer-helper-codec",
  "version": "2.4.30",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "sideEffects":false,
  "scripts": {
    "prepare": "npm run build",
    "watch": "npm run watch:es & npm run watch:lib",
    "watch:es": "babel src  --out-dir es --watch",
    "watch:lib": "babel src --presets @babel/preset-env --out-dir lib  --watch & tsc --watch",
    "build": "npm run build:es && npm run build:lib",
    "build:es": "babel src  --out-dir es && tsc",
    "build:lib": "babel src --presets @babel/preset-env --out-dir lib  && tsc --module CommonJS --outDir lib"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "xgplayer-helper-utils": "2.4.30"
  }
}
