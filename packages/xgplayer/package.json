{
    "name": "xgplayer",
    "version": "2.3.5",
    "description": "video player",
    "main": "./dist/index.min.js",
    "module": "./es/index.min.js",
    "typings": "./types/index.d.ts",
    "bin": {
        "xgplayer": "bin/xgplayer.js"
    },
    "scripts": {
        "prepare": "npm run build",
        "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
        "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
    },
    "keywords": [
        "video",
        "player"
    ],
    "babel": {
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "chrome": 58,
                        "ie": 10
                    },
                    "modules": false,
                    "useBuiltIns": "usage",
                    "corejs": 3
                }
            ]
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bytedance/xgplayer.git"
    },
    "author": "yinguohui@bytedance.com",
    "license": "MIT",
    "browserslist": [
        "> 5%",
        "IE 9",
        "iOS 7",
        "Firefox > 20"
    ],
    "devDependencies": {
        "@types/events": "^3.0.0",
        "autoprefixer": "^9.1.5",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-bulk-import": "^1.0.2",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "chai": "^4.1.2",
        "core-js": "^2.5.4",
        "cross-env": "^6.0.3",
        "node-sass": "^4.8.3",
        "postcss-cssnext": "^3.1.0",
        "sugarss": "^1.0.1",
        "rollup": "^1.27.0",
        "chalk": "^2.3.2",
        "fs-extra": "^5.0.0",
        "commander": "^2.15.1",
        "zlib": "^1.0.5"
    },
    "dependencies": {
        "danmu.js": "0.1.0",
        "deepmerge": "^1.5.0",
        "downloadjs": "1.4.7",
        "draggabilly": "^2.2.0",
        "event-emitter": "^0.3.5",
        "pasition": "^1.0.1",
        "request-frame": "^1.5.3"
    }
}
