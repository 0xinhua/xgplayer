{
    "name": "xgplayer",
    "version": "2.3.5",
    "description": "video player",
    "main": "./dist/index.min.js",
    "module": "./es/index.min.js",
    "typings": "./types/index.d.ts",
    "bin": {
        "xgplayer": "bin/xgplayer.js"
    },
    "scripts": {
        "prepare": "npm run build",
        "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
        "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
    },
    "keywords": [
        "video",
        "player"
    ],
    "babel": {
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "chrome": 58,
                        "ie": 10
                    },
                    "modules": false,
                    "useBuiltIns": "usage",
                    "corejs": 3
                }
            ]
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bytedance/xgplayer.git"
    },
    "author": "yinguohui@bytedance.com",
    "license": "MIT",
    "browserslist": [
        "> 5%",
        "IE 9",
        "iOS 7",
        "Firefox > 20"
    ],
    "dependencies": {
        "cross-env": "^6.0.3",
        "danmu.js": "0.1.0",
        "deepmerge": "^1.5.0",
        "delegate-events": "^1.1.1",
        "downloadjs": "1.4.7",
        "draggabilly": "^2.2.0",
        "event-emitter": "^0.3.5",
        "http-server": "^0.12.0",
        "pasition": "^1.0.1",
        "request-frame": "^1.5.3",
        "rollup": "^1.27.14",
        "rollup-plugin-uglify-es": "^0.0.1"
    }
}
