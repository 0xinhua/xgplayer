{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "chrome": 58,
                    "ie": 10
                },
                "modules": false,
                "useBuiltIns": "usage",
                "corejs": 3
            }
        ]
    ],
    "plugins": [
        [
            "inline-svg",
            {
                "svgo": {
                    "plugins": [
                        {
                            "removeViewBox": false,
                            "removeDimensions": true
                        }
                    ]
                }
            }
        ],
        "transform-class-properties"
    ]
}
