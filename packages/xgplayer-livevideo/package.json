{
  "name": "xgplayer-livevideo",
  "version": "3.0.0-rc.2",
  "description": "",
  "browser":"dist/index.min.js",
  "main": "dist/index.min.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "rm -rf ./es && babel src --out-dir es",
    "build:lib": "rm -rf ./lib &&babel src --presets @babel/preset-env --out-dir lib",
    "dev": "rollup -c rollup.config.js",
    "analyze": "cross-env NODE_ENV=production ANALYZE=1 rollup -c rollup.config.js",
    "watch": "babel src --out-dir es --watch & babel src --out-dir lib --watch & npm run watch:umd",
    "watch:umd": "rollup -c rollup.config.js --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "xgplayer-helper-utils": "3.0.0-rc.2",
    "xgplayer-helper-transmuxers": "3.0.0-rc.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ]
  }
}
