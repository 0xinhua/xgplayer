{
  "name": "xgplayer-hls.js",
  "version": "2.1.4",
  "description": "xgplayer plugin for hls",
  "main": "./dist/index.js",
  "scripts": {
    "build": "webpack --progress --display-chunks -p",
    "watch": "webpack --config webpack.config.dev.js --progress --display-chunks --mode development --watch",
    "dev": "webpack --progress --display-chunks --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "keywords": [
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "2.0.1",
    "event-emitter": "^0.3.5"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.10"
  }
}
