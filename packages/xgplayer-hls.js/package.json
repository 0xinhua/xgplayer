{
  "name": "xgplayer-hls.js",
  "version": "2.1.4",
  "description": "xgplayer plugin for hls",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "webpack --progress --display-chunks -p",
    "watch": "webpack --progress --display-chunks -p --watch",
    "dev": "webpack --progress --display-chunks --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "keywords": [
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^6.0.3",
    "deepmerge": "2.0.1",
    "event-emitter": "^0.3.5",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-uglify-es": "^0.0.1"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  }
}
