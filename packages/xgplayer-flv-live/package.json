{
  "name": "xgplayer-flv-live",
  "version": "3.0.0-rc.2",
  "description": "",
  "main": "dist/index.min.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:umd && npm run build:es",
    "build:types": "tsc src/index.js --allowJs --declaration --declarationDir types --emitDeclarationOnly",
    "analyze": "cross-env NODE_ENV=production ANALYZE=1 npm run build:umd && npm run build:es",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "rm -rf es && babel src --out-dir es",
    "build:lib": "rm -rf lib && babel src --presets @babel/preset-env --out-dir lib",
    "watch": "cross-env NODE_ENV=development rollup -c ./rollup.config.js --watch"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "search-and-replace",
        {
          "rules": [
            {
              "search": "__VERSION__",
              "searchTemplateStrings": true,
              "replace": "3.0.0-rc.2"
            }
          ]
        }
      ]
    ]
  },
  "author": "",
  "license": "ISC",
  "unpkgFiles": [
    "dist"
  ],
  "dependencies": {
    "xgplayer-helper-codec": "3.0.0-rc.2",
    "xgplayer-helper-models": "3.0.0-rc.2",
    "xgplayer-helper-transmuxers": "3.0.0-rc.2",
    "xgplayer-helper-utils": "3.0.0-rc.2"
  },
  "peerDependencies": {
    "xgplayer": "3.0.0-rc.2"
  }
}