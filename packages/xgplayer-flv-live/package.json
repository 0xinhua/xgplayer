{
  "name": "xgplayer-flv-live",
  "version": "2.2.0-alpha.55",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js && cross-env NODE_ENV=production rollup -c rollup.config.mobile.js ",
    "build:es": "rm -rf es && babel src --out-dir es",
    "build:lib": "rm -rf lib && babel src --out-dir lib --presets=es2015",
    "watch": "cross-env NODE_ENV=development rollup -c ./rollup.config.js --watch",
    "watch:mobile": "cross-env NODE_ENV=development rollup -c ./rollup.config.mobile.js --watch"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false
        }
      ]
    ]
  },
  "dependencies": {
    "xgplayer-utils-mse": "^2.2.0-alpha.55",
    "xgplayer-transmuxer-buffer-track": "^2.2.0-alpha.55",
    "xgplayer-transmuxer-buffer-presource": "^2.2.0-alpha.55",
    "xgplayer-transmuxer-buffer-xgbuffer": "^2.2.0-alpha.55",
    "xgplayer-transmuxer-loader-fetch": "^2.2.0-alpha.55",
    "xgplayer-transmuxer-demux-flv": "^2.2.0-alpha.55",
    "xgplayer-transmuxer-remux-mp4": "^2.2.0-alpha.55",
    "xgplayer-transmuxer-constant-events": "^2.2.0-alpha.55",
    "xgplayer-transmuxer-codec-compatibility": "^2.2.0-alpha.55",
    "xgplayer-utils-sniffer": "^2.2.0-alpha.55",
    "xgplayer-mobilevideo": "^2.2.0-alpha.55"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
