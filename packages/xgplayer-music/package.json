{
  "name": "xgplayer-music",
  "version": "2.0.1-beta.1",
  "description": "xgplayer plugin for music",
  "main": "./dist/index.min.js",
  "module": "./es/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false,
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "keywords": [
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-bulk-import": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "node-sass": "^4.9.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2"
  }
}
