{
  "name": "xgplayer-music",
  "version": "2.0.1-beta.1",
  "description": "xgplayer plugin for music",
  "main": "./dist/index.min.js",
  "module": "./es/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false,
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "keywords": [
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^6.0.3",
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-uglify-es": "^0.0.1"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  }
}
