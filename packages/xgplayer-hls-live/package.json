{
  "name": "xgplayer-hls-live",
  "version": "3.0.0-alpha.13",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "babel src --out-dir es",
    "build:lib": "babel src --out-dir lib --presets=env",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch",
    "watch:mobile": "cross-env NODE_ENV=development rollup -c ./rollup.config.mobile.js --watch"
  },
  "author": "",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false
        }
      ]
    ]
  },
  "license": "ISC",
  "dependencies": {
    "xgplayer-utils": "3.0.0-alpha.13",
    "xgplayer-utils-crypto": "3.0.0-alpha.13",
    "xgplayer-utils-mse": "3.0.0-alpha.13",
    "xgplayer-transmuxer-constant-events": "3.0.0-alpha.13",
    "xgplayer-transmuxer-context": "3.0.0-alpha.13",
    "xgplayer-transmuxer-buffer-track": "3.0.0-alpha.13",
    "xgplayer-transmuxer-buffer-presource": "3.0.0-alpha.13",
    "xgplayer-transmuxer-buffer-xgbuffer": "3.0.0-alpha.13",
    "xgplayer-transmuxer-buffer-stream": "3.0.0-alpha.13",
    "xgplayer-transmuxer-loader-fetch": "3.0.0-alpha.13",
    "xgplayer-transmuxer-remux-mp4": "3.0.0-alpha.13",
    "xgplayer-transmuxer-codec-compatibility": "3.0.0-alpha.13",
    "xgplayer-transmuxer-demux-m3u8": "3.0.0-alpha.13",
    "xgplayer-transmuxer-demux-ts": "3.0.0-alpha.13",
    "xgplayer-transmuxer-buffer-playlist": "3.0.0-alpha.13",
    "xgplayer-transmuxer-codec-avc": "3.0.0-alpha.13"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
