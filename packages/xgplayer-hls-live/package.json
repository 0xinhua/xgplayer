{
  "name": "xgplayer-hls-live",
  "version": "2.2.0-alpha.0",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "babel src --out-dir es",
    "build:lib": "babel src --out-dir lib --presets=env",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "author": "",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false
        }
      ]
    ]
  },
  "license": "ISC",
  "dependencies": {
    "xgplayer-loader": "^2.2.0-alpha.0",
    "xgplayer-buffer": "^2.2.0-alpha.0",
    "xgplayer-codec": "^2.2.0-alpha.0",
    "xgplayer-demux": "^2.2.0-alpha.0",
    "xgplayer-remux": "^2.2.0-alpha.0",
    "xgplayer-utils": "^2.2.0-alpha.0",
    "xgplayer-polyfills": "^2.2.0-alpha.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
