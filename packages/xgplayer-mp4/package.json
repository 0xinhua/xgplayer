{
  "name": "xgplayer-mp4",
  "version": "1.1.8",
  "description": "xgplayer plugin for mp4 transform to fmp4",
  "main": "./dist/index.min.js",
  "module": "./dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false,
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "keywords": [
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "concat-typed-array": "^1.0.2",
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-bulk-import": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "json-loader": "^0.5.7",
    "karma": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-chrome-launcher": "^2.2.0",
    "karma-webpack": "^4.0.0-rc.1",
    "mocha": "^5.2.0",
    "webpack": "^4.11.0"
  }
}
