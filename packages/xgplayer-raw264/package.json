{
  "name": "xgplayer-raw264",
  "version": "3.0.0-rc.1",
  "description": "",
  "browser":"dist/index.min.js",
  "main": "dist/index.min.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "rm -rf es && babel src --out-dir es",
    "build:lib": "rm -rf lib && babel src --presets @babel/preset-env --out-dir lib",
    "watch": "cross-env NODE_ENV=development rollup -c ./rollup.config.js --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "xgplayer-helper-codec": "3.0.0-rc.1",
    "xgplayer-helper-models": "3.0.0-rc.1",
    "xgplayer-helper-utils": "3.0.0-rc.1",
    "xgplayer-livevideo": "3.0.0-rc.1"
  }
}
