{
  "name": "xgplayer-raw264",
  "version": "2.3.29",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "./types/index.d.ts",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "rm -rf es && babel src --out-dir es",
    "build:lib": "rm -rf lib && babel src --out-dir lib --presets=es2015",
    "watch": "cross-env NODE_ENV=development rollup -c ./rollup.config.js --watch"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false
        }
      ]
    ]
  },
  "dependencies": {
    "xgplayer-mobilevideo": "2.3.27",
    "xgplayer-helper-utils": "2.3.26",
    "xgplayer-helper-codec": "2.3.26",
    "xgplayer-helper-models": "2.3.26"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
