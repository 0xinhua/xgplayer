{
  "name": "xgplayer-flv",
  "version": "2.2.0-beta.12",
  "description": "xgplayer transmuxer for FLV",
  "main": "./dist/index.min.js",
  "module": "./es/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance/xgplayer.git"
  },
  "keywords": [
    "flv",
    "player",
    "video",
    "live"
  ],
  "author": "fuyuhao@bytedance.com",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "cross-env": "^6.0.3"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  },
  "dependencies": {
    "xgplayer-flv-live": "^2.2.0-alpha.0",
    "xgplayer-flv-vod": "^2.2.0-alpha.0"
  }
}
